{
    "swagger": "2.0",
    "info": {
      "title": "My API",
      "description": "My API routes for my contacts list",
      "version": "1.0.0"
    },
    "host": "cse-341-web-services-imke.onrender.com",
    "basePath": "/",
    "schemes": [
      "http", "https"
    ],
    "paths": {
      "/": {
        "get": {
          "description": "",
          "responses": {
            "200": {
              "description": "OK"
            }
          }
        }
      },
      "/contacts/": {
        "get": {
          "description": "Get all contacts from the database",
          "produces": [
            "application/json"
          ],
          "responses": {
            "200": {
              "description": "OK"
            }
          }
        },
        "post": {
          "description": "Create contact in the database",
          "parameters": [
            {
              "name": "body",
              "in": "body",
              "schema": {
                "type": "object",
                "properties": {
                  "firstName": {
                    "example": "any"
                  },
                  "lastName": {
                    "example": "any"
                  },
                  "email": {
                    "example": "any"
                  },
                  "favoriteColor": {
                    "example": "any"
                  },
                  "birthday": {
                    "example": "any"
                  }
                }
              }
            }
          ],
          "responses": {
            "201": {
              "description": "Created"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        }
      },
      "/contacts/{id}": {
        "get": {
          "description": "Find contacts by ID",
          "produces": [
            "application/json"
          ],
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "OK"
            },
            "404": {
              "description": "Not Found"
            }
          }
        },
        "put": {
          "description": "Update my contacts list by ID",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "type": "string"
            },
            {
              "name": "body",
              "in": "body",
              "schema": {
                "type": "object",
                "properties": {
                  "firstName": {
                    "example": "any"
                  },
                  "lastName": {
                    "example": "any"
                  },
                  "email": {
                    "example": "any"
                  },
                  "favoriteColor": {
                    "example": "any"
                  },
                  "birthday": {
                    "example": "any"
                  }
                }
              }
            }
          ],
          "responses": {
            "204": {
              "description": "No Content"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        },
        "delete": {
          "description": "Delete single contact by ID",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "204": {
              "description": "No Content"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        }
      }
    }
  }